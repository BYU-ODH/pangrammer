;; Pangrammer shadow-cljs configuration
{:source-paths
 ["src/dev"
  "src/main"
  "src/test"]

 :dependencies [[reagent "0.10.0"]
                [metosin/reitit-frontend "0.3.9"]]

 :builds
 {:app {:output-dir "target/"
        :asset-path "."
        :target :browser
        :modules {:main {:init-fn pangrammer.core/init!}}
        :devtools {:after-load pangrammer.core/init!
                   :http-root "target"
                   :http-port 8080}}}}
